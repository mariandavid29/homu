{
  "name": "homu",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "pnpm run -r --parallel --aggregate-output dev",
    "build": "pnpm run build:packages && pnpm run validate && pnpm run build:apps",
    "build:packages": "pnpm --filter './packages/*' -r build",
    "build:apps": "pnpm --filter './apps/*' -r build",
    "deploy": "pnpm build && pnpm run -r deploy",
    "validate": "pnpm lint && pnpm typecheck",
    "typecheck": "pnpm run -r --parallel typecheck --noEmit",
    "lint": "pnpm run -r --parallel lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.19.0"
}
